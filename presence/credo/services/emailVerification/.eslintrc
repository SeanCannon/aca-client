{
    "env": {
      "node": true,
      "jasmine": true
    },
    "extends": [
      "standard"
    ],
    "rules": {
      "spaced-comment": 0,
      "no-multi-spaces": 0,
      "operator-linebreak": 0,
      "comma-dangle": 0,
      "key-spacing": 0,
      "indent": [
        "error",
        2,
        { "VariableDeclarator": { "var": 2, "let": 2, "const": 3 } }
      ],
      "one-var": 0,
      "semi": [
        2,
        "always"
      ]
    }
  }